{
  "name": "mathematiks",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:api": "cd api && uv run uvicorn src.main:app --host 0.0.0.0 --port 8000 --reload",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openapi:fetch": "curl http://localhost:8000/openapi.json -o openapi.json",
    "openapi:generate": "openapi-typescript openapi.json -o ./lib/api/schema.ts",
    "openapi": "bun run openapi:fetch && bun run openapi:generate"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "next": "15.4.4",
    "openapi-fetch": "^0.14.1",
    "openapi-typescript": "^7.9.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "@eslint/eslintrc": "^3"
  }
}
